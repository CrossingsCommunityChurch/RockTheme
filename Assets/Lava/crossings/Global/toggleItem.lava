<div class="toggle-item bg-white shadowDepth2 mt-2">
  <a class="toggle-title collapsed" role="button" data-toggle="collapse" href="#{{identifier}}" aria-expanded="false" aria-controls="{{identifier}}">{% if icon != '' %}<i class="fa {{icon}}"></i>{% endif %}<span>{{title}}</span><i class="fa fa-angle-down angle-toggle"></i></a>
  <div class="toggle-content collapse" id="{{identifier}}">
      <div class="p-2">
          {{ content }}
      </div>
  </div>
</div>
